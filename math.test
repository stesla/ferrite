#!/bin/sh

echo "1..1"

if [ "$(./fe - <<EOF
;; Environment
NIL

;; Code
(NIL

 ;; Integer Division (order matters)
 LDC 9 LDC 3 DIV CONS
 LDC 3 LDC 9 DIV CONS

 ;; Multiplication (order does not matter)
 LDC 6 LDC 7 MUL CONS
 LDC 7 LDC 6 MUL CONS

 ;; Subtraction (order matters)
 LDC 6 LDC 4 SUB CONS
 LDC 4 LDC 6 SUB CONS

 PRINT)
EOF
)" == "(2 -2 42 42 3 0)" ]; then
    echo "ok 1"
else
    echo "not ok 1"
fi